{
  "routes": [
    {
      "route": "/",
      "redirect": "/landing",
      "statusCode": 302
    },
    {
      "route": "/landing",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/stockbase",
      "allowedRoles": [
        "authenticated"
      ]
    }
  ],
  "auth": {
    "identityProviders": {
      "azureActiveDirectory": {
        "registration": {
          "openIdIssuer": "https://login.microsoftonline.com/c41916b0-5719-48ce-929a-2d4305f25880/v2.0",
          "clientIdSettingName": "VITE_CLIENT_ID"
        },
        "login": {
          "loginParameters": [
            "scope=openid profile email"
          ]
        }
      }
    }
  },
  "globalHeaders": {
    "X-Frame-Options": "DENY",
    "X-Content-Type-Options": "nosniff"
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*"
    ]
  }
}